      <TableView fx:id="accountsTableView" layoutX="10.0" layoutY="10.0" prefHeight="500.0" prefWidth="700.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
         <columns>
            <TableColumn fx:id="idColumn" prefWidth="75.0" text="ID" />
            <TableColumn fx:id="accNameColumn" minWidth="200.0" prefWidth="75.0" text="Account Name" />
            <TableColumn fx:id="passwordColumn" prefWidth="75.0" text="Password" />
            <TableColumn fx:id="roleColumn" minWidth="90.0" prefWidth="75.0" text="Role" />
         </columns>
      </TableView>
   </children>
</AnchorPane>
